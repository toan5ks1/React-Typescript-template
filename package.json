{
    "name": "school-portal",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "NODE_OPTIONS=--max-old-space-size=8192 vite",
        "dev:debug": "vite --debug",
        "build:vite": "node --max_old_space_size=16384 ./node_modules/.bin/vite build",
        "start": "env-cmd -f environments/.env yarn dev",
        "start:eibanam": "env-cmd -f environments/.env yarn dev",
        "start:synersia": "env-cmd -f environments/.env.synersia yarn dev",
        "start:jprep": "env-cmd -f environments/.env.jprep yarn dev",
        "build": "export VITE_BUILD_VERSION=$(echo $FE_TAG | sed 's|/|-|g') && yarn build:vite",
        "serve": "vite preview --port 3001",
        "deploy:manabie:staging": "env-cmd -f environments/.env.manabie.staging yarn build && firebase deploy --only hosting:green --project staging-manabie-online",
        "deploy:manabie:uat": "env-cmd -f environments/.env.manabie.uat yarn build && firebase deploy --only hosting:uat-manabie --project uat-manabie",
        "deploy:muiv5": "env-cmd -f environments/.env.manabie.staging yarn build && firebase deploy --only hosting:dev-mui-v5 --project student-coach-e1e95",
        "firebase-deploy:manabie:staging": "firebase deploy --only hosting:green --project staging-manabie-online",
        "firebase-deploy:manabie:uat": "firebase deploy --only hosting:uat-manabie --project uat-manabie",
        "firebase-deploy:jprep:staging": "firebase deploy --only hosting:staging-jprep --project staging-manabie-online",
        "firebase-deploy:jprep:uat": "firebase deploy --only hosting:uat-jprep --project staging-manabie-online",
        "firebase-deploy:jprep:production": "firebase deploy --only hosting:production-jprep --project live-manabie",
        "firebase-deploy:synersia:production": "firebase deploy --only hosting:synersia --project synersia",
        "firebase-deploy:renseikai:production": "firebase deploy --only hosting:production-renseikai --project production-renseikai",
        "firebase-deploy:ga:production": "firebase deploy --only hosting:production-ga --project production-ga",
        "deploy:synersia:production": "env-cmd -f environments/.env.synersia.production yarn build && firebase deploy --only hosting:synersia --project synersia",
        "deploy:jprep:staging": "env-cmd -f environments/.env.jprep.staging yarn build && firebase deploy --only hosting:staging-jprep --project staging-manabie-online",
        "deploy:jprep:uat": "env-cmd -f environments/.env.jprep.uat yarn build && firebase deploy --only hosting:uat-jprep --project staging-manabie-online",
        "deploy:jprep:production": "env-cmd -f environments/.env.jprep.production yarn build && firebase deploy --only hosting:production-jprep --project live-manabie",
        "deploy:renseikai:production": "env-cmd -f environments/.env.renseikai.production yarn build && firebase deploy --only hosting:production-renseikai --project production-renseikai",
        "deploy:ga:production": "env-cmd -f environments/.env.ga.production yarn build && firebase deploy --only hosting:production-ga --project production-ga",
        "deploy:aic:preproduction": "env-cmd -f environments/.env.aic.preproduction yarn build",
        "deploy:ga:preproduction": "env-cmd -f environments/.env.ga.preproduction yarn build",
        "deploy:jprep:preproduction": "env-cmd -f environments/.env.jprep.preproduction yarn build",
        "deploy:renseikai:preproduction": "env-cmd -f environments/.env.renseikai.preproduction yarn build",
        "deploy:synersia:preproduction": "env-cmd -f environments/.env.synersia.preproduction yarn build",
        "test": "NODE_OPTIONS='--unhandled-rejections=strict' node --trace-uncaught --expose-gc --optimize-for-size ./node_modules/.bin/jest --logHeapUsage",
        "test:unit-ci": "CI=true node ./scripts/run-unit.js",
        "test:unit:cheap": "yarn test -o --watchAll=false --maxWorkers=4",
        "test:unit": "NODE_ENV=test && node node_modules/.bin/jest",
        "test:unit:debug": "NODE_OPTIONS='--unhandled-rejections=strict' node --expose-gc --optimize-for-size ./node_modules/.bin/jest --runInBand --verbose=false --logHeapUsage",
        "test:selective": "yarn test --config=jest.selective.js",
        "translation-check": "yarn mana script translation check",
        "typecheck:unit": "./node_modules/.bin/tsc -p tsconfig.unit.json",
        "typecheck:unit-all": "node ./scripts/run-unit-typecheck.js",
        "typecheck:unit-squad": "./node_modules/.bin/tsc -p src/squads/${project}/tsconfig.unit.json",
        "ts-to-json": "yarn mana script ts-json",
        "metadata:export": "yarn mana hasura metadata export",
        "metadata:export:local": "yarn mana hasura metadata export --use-local true",
        "metadata:import:local": "yarn mana hasura metadata import",
        "graphql:export": "yarn mana graphql query export",
        "schema:export": "yarn mana hasura schema export",
        "schema:export:local": "yarn mana hasura schema export --use-local true",
        "typescript:export": "yarn schema:export:local && yarn mana graphql ts export",
        "allow-list:update": "yarn mana hasura allow-list update",
        "allow-list": "yarn metadata:export:local && yarn allow-list:update",
        "postinstall": "husky install && node ./node_modules/vite-plugin-shim-react-pdf/scripts/shim-react-pdf.js",
        "eslint:check": "eslint . --max-warnings=0 --ext .ts,.tsx --cache --cache-location .eslintcache",
        "eslint:fix": "yarn eslint:check --fix",
        "prettier:check": "prettier '**/*.{ts?(x),js?(on)}' --check ",
        "prettier:fix": "prettier '**/*.{ts?(x),js?(on)}' --write",
        "lint": "yarn typecheck:unit && yarn prettier:check && yarn eslint:check",
        "depcruise:root": "depcruise --config ./scripts/depcruise-strict/depcruise-root.js --include-only '^src' ./src",
        "eslint:check:folder": "eslint ${FOLDER} --max-warnings=0 --ext .ts,.tsx --cache --cache-location .eslintcache"
    },
